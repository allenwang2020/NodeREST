{
	  "AWSEBDockerrunVersion": 2,
	  "volumes": [
	             
	    {
	      "name": "web-node",
	      "host": {
	        "sourcePath": "/var/app/current/web"
	      }
	    }
	  ],
	  "containerDefinitions": [
		                                                  
	    {
	      "name": "web-node",
	      "image": "node:4.4.2",
	      "essential": true,
	      "memory": 128,
	      "portMappings": [
	        {
	          "hostPort": 3000,
	          "containerPort": 3000
	        }
	      ],
	      "command": [
	        "/bin/bash",
	        "/usr/src/app/staging/web/run.sh"
	      ],
	      "mountPoints": [
	        {
	          "sourceVolume": "web-node",
	          "containerPath": "/usr/src/app/staging/web"
	        }
	      ]
	    }
	  ]
}