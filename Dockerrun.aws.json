{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "node-web",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "27017"
    },
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/mongodb",
      "ContainerDirectory": "/etc/mongodb"
    },
    {
      "HostDirectory": "/var/app/current",
      "ContainerDirectory": "/usr/src"
    }
  ],
  "Logging": "/var/log/nginx"
}