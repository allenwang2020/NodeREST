{
	  "AWSEBDockerrunVersion": 2,
	  "volumes": [
	    {
	      "name": "node-web",
	      "host": {
	        "sourcePath": "/var/app/current"
	      }
	    },
	    {
	    
	      "name": "mongodb-data",
	      "host": {
	        "sourcePath": "/var/app/current/mongodb-data"
	      }
	    }
	  ],
	  "containerDefinitions": [
	    {
	      "name": "node-web",
	      "image": "node:4.4.2",
	      "essential": true,
	      "memory": 128,
	      "portMappings": [
	        {
	          "hostPort": 80,
	          "containerPort": 8080
	        }
	      ],
	      "mountPoints": [
	        {
	          "sourceVolume": "node-web",
	          "containerPath": "/usr/src/app"
	        }
	      ],
	      "command": 
	      [
	     	 "/bin/bash",
	      	 "/usr/src/app/run_node.sh"
	      ]
	    }
	  ]
}